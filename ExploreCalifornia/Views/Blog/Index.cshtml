@model IEnumerable<ExploreCalifornia.Models.Post>
@{
    Layout = "_Layout";
}

<h1>Explorer's Blog</h1>

<p>
    Below are a few of the latest posts from some of our explorers.
</p>

<div class="blog-posts">
    @foreach (var post in Model)
    {
        @await Html.PartialAsync("_Post", post)
        @*defualtne berie partial view rovnaky model ako parent view, ale je mozne prepisat 2 parametrom*@
    }
</div>